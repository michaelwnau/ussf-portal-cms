  @@index([articleTagId])
  @@index([updatedById])
  @@index([createdById])
}

enum UserRoleType {
  User
  Author
  Manager
}

enum LabelTypeType {
  Source
  Audience
  Base
}

enum ArticleCategoryType {
  InternalNews
  ORBITBlog
  LandingPage
}

enum ArticleStatusType {
  Draft
  Published
  Archived
}

enum AnnouncementStatusType {
  Draft
  Published
  Archived
}

enum LandingPageStatusType {
  Draft
  Published
  Archived
}
